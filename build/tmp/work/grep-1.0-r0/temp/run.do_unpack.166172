#!/bin/sh

# Emit a useful diagnostic if something fails:
bb_exit_handler() {
    ret=$?
    case $ret in
    0)  ;;
    *)  case $BASH_VERSION in
        "") echo "WARNING: exit code $ret from a shell command.";;
        *)  echo "WARNING: ${BASH_SOURCE[0]}:${BASH_LINENO[0]} exit $ret from '$BASH_COMMAND'";;
        esac
        exit $ret
    esac
}
trap 'bb_exit_handler' 0
set -e
export HOME="/home/eurekaserver"
export LC_ALL="en_US.UTF-8"
export LOGNAME="eurekaserver"
export PATH="/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/snap/bin:/home/eurekaserver/yocto/base/poky/bitbake/bin"
export SHELL="/bin/bash"
export TERM="xterm"
export USER="eurekaserver"

do_unpack() {
    for i in $(ls -1 "/home/eurekaserver/yocto/base/build/tmp/work/grep-1.0-r0/wget")
    do
        tar -xf /home/eurekaserver/yocto/base/build/tmp/work/grep-1.0-r0/wget/$i
    done
}

do_unpack

# cleanup
ret=$?
trap '' 0
exit $ret
